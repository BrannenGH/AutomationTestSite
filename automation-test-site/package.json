{
  "name": "@cognizant-softvision/automation-test-site",
  "version": "0.0.1",
  "private": true,
  "description": "Automation Test Site",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build && shx cp -r static/* public/",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "push": "gatsby build --prefix-paths && gh-pages -f -d public",
    "docker-build": "docker build -t automation-test-site --label automation-test-site .",
    "docker-run": "docker run -d -p 8080:80 --name automation-test-suite automation-test-site",
    "docker-stop": "docker container stop automation-test-suite && docker rm automation-test-suite"
  },
  "dependencies": {
    "gatsby": "^4.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "shx": "^0.3.4"
  }
}
